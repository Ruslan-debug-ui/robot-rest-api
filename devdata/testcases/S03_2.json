{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "description": {
            "type": "string"
        },
        "settings": {
            "type": "object",
            "properties": {
                "upload_url": {
                    "type": "string"
                },
                "time_url": {
                    "type": "string"
                }
            },
            "additionalProperties": false,
            "required": [
                "upload_url",
                "time_url"
            ]
        },
        "project_id": {
            "type": "integer"
        },
        "name": {
            "type": "string"
        },
        "id": {
            "type": "integer"
        },
        "created_at": {
            "type": "string"
        },
        "updated_at": {
            "type": "string"
        }
    },
    "additionalProperties": false,
    "required": [
        "description",
        "settings",
        "project_id",
        "name",
        "id",
        "created_at",
        "updated_at"
    ]
}