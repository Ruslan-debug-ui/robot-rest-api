{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "access_token": {
            "type": "string"
        },
        "refresh_token": {
            "type": "string"
        },
        "token_type": {
            "type": "string"
        },
        "user": {
            "type": "object",
            "properties": {
                "role": {
                    "type": "string"
                },
                "household_id": {
                    "type": "null"
                },
                "settings": {
                    "type": "object",
                    "additionalProperties": false
                },
                "description": {
                    "type": "null"
                },
                "is_active": {
                    "type": "boolean"
                },
                "is_synced": {
                    "type": "boolean"
                },
                "meter_settings": {
                    "type": "object",
                    "additionalProperties": false
                },
                "id": {
                    "type": "integer"
                },
                "username": {
                    "type": "string"
                },
                "group_id": {
                    "type": "null"
                },
                "created_at": {
                    "type": "string"
                },
                "updated_at": {
                    "type": "string"
                },
                "devices": {
                    "type": "array"
                },
                "last_active": {
                    "type": "string"
                },
                "last_app_version": {
                    "type": "null"
                },
                "last_device_id": {
                    "type": "null"
                },
                "is_online": {
                    "type": "boolean"
                }
            },
            "additionalProperties": false,
            "required": [
                "role",
                "settings",
                "is_active",
                "is_synced",
                "meter_settings",
                "id",
                "username",
                "created_at",
                "updated_at",
                "devices",
                "last_active",
                "is_online"
            ]
        }
    },
    "additionalProperties": false,
    "required": [
        "access_token",
        "refresh_token",
        "token_type",
        "user"
    ]
}