{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "array",
    "items": {
        "type": "object",
        "properties": {
            "role": {
                "type": "string"
            },
            "household_id": {
                "type": "integer"
            },
            "settings": {
                "type": "object",
                "properties": {
                    "upload_url": {
                        "type": "string"
                    },
                    "time_url": {
                        "type": "string"
                    }
                },
                "additionalProperties": false,
                "required": [
                    "upload_url",
                    "time_url"
                ]
            },
            "description": {
                "type": "string"
            },
            "is_active": {
                "type": "boolean"
            },
            "is_synced": {
                "type": "boolean"
            },
            "meter_settings": {
                "type": "object",
                "properties": {
                    "plug_power_threshold_lo": {
                        "type": "integer"
                    },
                    "plug_power_threshold_hi": {
                        "type": "number"
                    },
                    "no_viewers_mode_max_duration": {
                        "type": "integer"
                    },
                    "check_viewers_remind_freq": {
                        "type": "integer"
                    },
                    "register_remind_freq_0": {
                        "type": "integer"
                    },
                    "register_remind_period_0": {
                        "type": "integer"
                    },
                    "register_remind_freq_1": {
                        "type": "integer"
                    },
                    "register_remind_period_1": {
                        "type": "integer"
                    },
                    "unregister_all_delay": {
                        "type": "integer"
                    },
                    "warning_remind_freq": {
                        "type": "integer"
                    },
                    "register_remind_delay": {
                        "type": "integer"
                    }
                },
                "additionalProperties": false,
                "required": [
                    "plug_power_threshold_lo",
                    "plug_power_threshold_hi",
                    "no_viewers_mode_max_duration",
                    "check_viewers_remind_freq",
                    "register_remind_freq_0",
                    "register_remind_period_0",
                    "register_remind_freq_1",
                    "register_remind_period_1",
                    "unregister_all_delay",
                    "warning_remind_freq"
                ]
            },
            "id": {
                "type": "integer"
            },
            "username": {
                "type": "string"
            },
            "group_id": {
                "type": "integer"
            },
            "created_at": {
                "type": "string"
            },
            "updated_at": {
                "type": "string"
            },
            "devices": {
                "type": "array",
                "items": {
                    "type": "object",
                    "properties": {
                        "device_id": {
                            "type": "integer"
                        },
                        "guid": {
                            "type": "string"
                        },
                        "id": {
                            "type": "integer"
                        },
                        "created_at": {
                            "type": "string"
                        },
                        "updated_at": {
                            "type": "string"
                        }
                    },
                    "additionalProperties": false,
                    "required": [
                        "device_id",
                        "guid",
                        "id",
                        "created_at",
                        "updated_at"
                    ]
                }
            },
            "last_active": {
                "type": "null"
            },
            "last_app_version": {
                "type": "null"
            },
            "last_device_id": {
                "type": "null"
            },
            "is_online": {
                "type": "boolean"
            }
        },
        "additionalProperties": false,
        "required": [
            "role",
            "household_id",
            "settings",
            "description",
            "is_active",
            "is_synced",
            "meter_settings",
            "id",
            "username",
            "group_id",
            "created_at",
            "updated_at",
            "devices",
            "is_online"
        ]
    }
}