{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "categories": {
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "data": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "category_index": {
                        "type": "integer"
                    },
                    "start_time": {
                        "type": "string"
                    },
                    "finish_time": {
                        "type": "string"
                    }
                },
                "additionalProperties": false,
                "required": [
                    "category_index",
                    "start_time",
                    "finish_time"
                ]
            }
        }
    },
    "additionalProperties": false,
    "required": [
        "categories",
        "data"
    ]
}